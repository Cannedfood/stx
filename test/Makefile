# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=test
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),test)
  test.run_config = test

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := test.run

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

test.run:
ifneq (,$(test.run_config))
	@echo "==== Building test.run ($(test.run_config)) ===="
	@${MAKE} --no-print-directory -C . -f test.run.make config=$(test.run_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f test.run.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  test"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   test.run"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"